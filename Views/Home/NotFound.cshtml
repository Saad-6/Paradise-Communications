@{
    ViewData["Title"] = "404 | Not Found";
}

<div class="d-flex justify-content-center align-items-center min-vh-100">
    <div class="text-center">
        <h1 class="text-success">Error : 404 Not Found</h1>
        <p>The page you were looking for does not exist .</p>
        <p>Either the url was incorrect or the page was removed</p>
        <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Return to Home</a>
    </div>
</div>
